<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="Giant">
		<soundtype name="Select">
			<soundset name="SeminoleSharktoothBowmanSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="LazerBearBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="SeminoleSharktoothBowmanAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="SeminoleSharktoothBowmanAttack"/>
				</choice>
				<choice name="build"/>
				<choice name="Huntable"/>
				<choice name="Tree"/>
				<choice name="AbstractMine"/>
				<choice name="AbstractFarm"/>
				<choice name="AbstractResourceCrate"/>
				<choice name="AbstractFruit"/>
			</targetlogic>
		</soundtype>
		<soundtype name="KnockOut">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="KnockOutRevived">
			<soundset name="WarChiefBirth"/>
		</soundtype>
		<soundtype name="Ransomed">
			<soundset name="WarChiefBirth"/>
		</soundtype>
	</protounit>
</protounitsounddef>