<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="Robot">
		<soundtype name="Select">
			<soundset name="GreekHalberdierSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MilitaryBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="GreekHalberdierAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="GreekHalberdierAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>