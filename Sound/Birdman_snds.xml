<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="Birdman">
		<soundtype name="Select">
			<soundset name="RussianMusketeerSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="CavalryGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="LazerBearBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="RussianMusketeerAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="RussianMusketeerAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
		<soundtype name="KnockOutRevived">
			<soundset name="WarChiefBirth"/>
		</soundtype>
		<soundtype name="Ransomed">
			<soundset name="WarChiefBirth"/>
		</soundtype>
	</protounit>
</protounitsounddef>